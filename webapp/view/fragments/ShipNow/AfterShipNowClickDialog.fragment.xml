<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog id="shipmentLabelDialog"
            contentWidth="500px"
            contentHeight="100%"
            verticalScrolling="false"
            horizontalScrolling="false"
             
       
        showHeader="false" 
        class="rightSidePopoverShipNowsideLabel">
            
        <customHeader>
            <Toolbar>
                <Title text="Shipment Label" level="H6" class="sapUiLargeMarginTop sapUiLargeMarginBottom" />
                <ToolbarSpacer/>
                <Button icon="sap-icon://decline" type="Transparent" press="onshipmentLabelDialogdelinePress"/>
            </Toolbar>
        </customHeader>

        <VBox alignItems="Center" justifyContent="Start">
            <Image id="shipmentLabelImage"
                   width="500px"
                   height="95%"
                   class="sapUiLargeMarginTop"
                   densityAware="false"/>
        </VBox>

        <footer>
            <OverflowToolbar class="p-sidebar-header sapMPopoverFooter1 shipmethod">
              
                <ToolbarSpacer />
                <Button type="Emphasized" text="Close" press="onshipmentLabelDialogSelectPress" />
            </OverflowToolbar>
        </footer>
    </Dialog>
</core:FragmentDefinition>
