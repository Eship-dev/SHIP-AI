<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog id="idAfterShipmentLabelDialog"
            contentWidth="30vh"
            contentHeight="95%"
            verticalScrolling="false"
            horizontalScrolling="false"
             
       
        showHeader="false"
        class="rightSidePopoverShipNowsideLabel1">
           
        <customHeader>
            <Toolbar>
                <Title text="Shipment Label" level="H6" class="sapUiLargeMarginTop sapUiLargeMarginBottom" />
                <ToolbarSpacer/>
                <Button icon="sap-icon://decline" type="Transparent" press=".onShipmentLabelDialogClosePress" class="Decline_Btn ship-now-decline_btn"/>
            </Toolbar>
        </customHeader>


        <VBox alignItems="Center" justifyContent="Center" >
            <Image id="shipmentLabelImage"
                   width="60vh"
                   height="70vh"
                   class="sapUiLargeMarginTop "
                   densityAware="false" />
        </VBox>
        <footer >
            <OverflowToolbar class="p-sidebar-header sapMPopoverFooter1" >
                <Button
                    text="Cancel"
                    class="save1-btn sapUiTinayMarginTop"
                    press="onShipmentLabelDialogClosePress" />
                   
                <ToolbarSpacer />
                <Button
                    type="Ghost"
                    class="scan-ship-toolbar1__button sapUiTinayMarginTop"
                    text="Print"
                />
            </OverflowToolbar>
        </footer>
    </Dialog>
</core:FragmentDefinition>
