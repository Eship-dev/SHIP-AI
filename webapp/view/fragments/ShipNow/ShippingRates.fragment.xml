<core:FragmentDefinition   xmlns="sap.m"  xmlns:core="sap.ui.core"   height="100%">
    <Dialog id="_IDGenShipNowShppindRateDialog" title="Pick a Carrier" titleAlignment="Center">
        <content>
            <Table id="_ShippingRateTableId"  inset="false"  mode="SingleSelectLeft"  items="{path: 'eshipjetModel>/shipNowShippingRates'}">               
                <columns>
                    <Column id="_IDGenColumn1">
                        <Text id="_IDGenText2" text="Carrier" />
                    </Column>
                    <Column id="_IDGenColumn2">
                        <Text id="_IDGenText3" text="Service Name" />
                    </Column>
                    <Column id="_IDGenColumn3" >
                        <Text id="_IDGenText4" text="Published Rate $" />
                    </Column>
                    <Column id="_IDGenColumn4" >
                        <Text id="_IDGenText5" text="Discount Rate $" />
                    </Column>
                    <Column id="_IDGenColumn5">
                        <Text id="_IDGenText6" text="Estimated Arrival" />
                    </Column>
                     <Column id="_IDGenColumn6">
                        <Text id="_IDGenText7" text="Transit Days" />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem id="_IDGenColumnListItem" vAlign="Middle">
                        <cells>
                            <Text id="_IDGenText8" text="{eshipjetModel>Carrier}"></Text>
                            <Text id="_IDGenText9" text="{eshipjetModel>serviceName}"></Text>
                            <Text id="_IDGenText10" text="{eshipjetModel>publishedFreight}"></Text>
                            <Text id="_IDGenText11" text="{eshipjetModel>discountFreight}"></Text>
                            <Text id="_IDGenText12" text="{eshipjetModel>expectedDeliveryDate}"></Text>
                            <Text id="_IDGenText13" text="{eshipjetModel>transitDays}"></Text>                          
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </content>
        <footer>
			<Toolbar>
				<content>	
                	<ToolbarSpacer />				
					<Button type="Accept"
							text="Submit"
							press=".onPickCarrierSubmit" />
					<Button id="Reject"
						text="Cancel"
						type="Reject"
						press=".onCloseShipNowShippinRateDialog"
						ariaHasPopup="Dialog" />
				</content>
			</Toolbar>
		</footer>
    </Dialog>
</core:FragmentDefinition>