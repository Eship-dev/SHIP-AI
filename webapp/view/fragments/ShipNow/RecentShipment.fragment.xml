<core:FragmentDefinition	xmlns="sap.m"    xmlns:ui="sap.ui.table"  	xmlns:l="sap.ui.layout"	xmlns:f="sap.ui.layout.form"	xmlns:core="sap.ui.core">
    <Popover	id="myPopoverRecentShipment"  contentWidth="30%" contentHeight="40%" placement="Bottom">  
		<customHeader>
			<VBox id="_IDGenVBoxRecentShipment">
				<Toolbar id="_IDGenHBoxRecentShipment" class="toolbarbottomborder">
					<Title  class="sapUiTinyMarginBegin sapUiTinyMarginTop" id="_IDGenTitleRecentShipment" text="Recent Shipments"></Title>
					<ToolbarSpacer id="_IDGenToolbarSpacer1RecentShipment" ></ToolbarSpacer>					
					<Button id="_IDCloseIconPopoverRecentShipment" icon="sap-icon://decline" press="onRecentShipmentClosePress" class="Decline_Btn" ></Button>
				</Toolbar>				
			</VBox>
		</customHeader>
		<content>
        <Table	id="rencentShipmentTblId"  items="{eshipjetModel>/RecentShipmentSet}" >
		<ColumnListItem  vAlign="Middle" press="onRecentShipmentsItemPress" type="Active" >
			<cells>				
			  	<ObjectStatus	text="{eshipjetModel>details/HeaderInfo/DocumentNumber}" state="Information"	class="customObjectStatus" />
                <Text text="{path: 'eshipjetModel>details/HeaderInfo/ShipDate', formatter:'Formatter.formatDate'}" />	
                <Text text="{eshipjetModel>details/HeaderInfo/ShipmentType}"  wrapping="false" />	
                <Text text="{eshipjetModel>details/CarrierDetails/Carrier}" />
			</cells>
		</ColumnListItem>
		<columns>	
        <Column ></Column>				
        <Column ></Column>				
        <Column ></Column>				
        <Column ></Column>				
		</columns>
        </Table>
            </content>        
	</Popover>   
</core:FragmentDefinition>
