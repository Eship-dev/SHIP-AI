<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:svm="sap.ui.comp.smartvariants"
    xmlns:ui="sap.ui.table"
	xmlns:u="sap.ui.unified"
	xmlns:c="sap.ui.core" >
    <Dialog
        id="idHandlingUnitDialog1"
        showHeader="false"
        contentWidth="70%"
        contentHeight="40%" >

        <Toolbar >
            <Text text="Product Details"></Text>
            <ToolbarSpacer />
            <Button icon="sap-icon://decline" press="onHandlingUnitDialogClosePress" class="Decline_Btn ship-now-decline_btn" ></Button>
        </Toolbar>

           
            <Table id="HandlingUnitTableId" items="{eshipjetModel>/handlingUnitItems}">
                <columns>
                    <Column>
                        <Text text="Item No"/>
                    </Column>
                    <Column>
                        <Text text="Product Code"/>
                    </Column>
                    <Column>
                        <Text text="Description"/>
                    </Column>
                    <Column>
                        <Text text="Quantity"/>
                    </Column>
                    <Column>
                        <Text text="UOM"/>
                    </Column>
                </columns>
                <items>
                    <ColumnListItem>
                        <cells>
                            <Text text="{eshipjetModel>Itemno}"/>
                            <Text text="{eshipjetModel>Delmatnr}"/>
                            <Text text="{eshipjetModel>Maktx}"/>
                            <Text text="{= parseFloat(${eshipjetModel>Qty}).toFixed() }"/>
                            <Text text="{eshipjetModel>Meins}"/>
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>

            <footer >
                    <Toolbar >
                       <ToolbarSpacer ></ToolbarSpacer>
                            <Button  text="First" />
                            <Button  text="Previous" />
                            <Text    text="51-64 of 64" />
                            <Button  text="Next" enabled="false" />
                            <Button  text="Last" enabled="false" />
                    </Toolbar>
            </footer>
    </Dialog>
</core:FragmentDefinition>
