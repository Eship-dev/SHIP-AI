<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core">
    <Dialog
        id="openDialog"
        title="Default Configuration"
    >
        <f:SimpleForm id="SimpleFormChange480_12120Dual"
            editable="true"
            layout="ResponsiveGridLayout"
            labelSpanXL="4"
            labelSpanL="4"
            labelSpanM="12"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="0"
            emptySpanL="0"
            emptySpanM="0"
            emptySpanS="0"
            columnsXL="2"
            columnsL="2"
            columnsM="2"
            singleContainerFullSize="false">
            <f:content>
                <Label text="Connect To" />
                <ComboBox width="100%" />
                <Label text="Order Type" />
                <ComboBox width="100%" />
                <Label text="Ship Method" />
                <ComboBox width="100%" />
            </f:content>
        </f:SimpleForm>

        <Toolbar>
            <ToolbarSpacer />
            <Button text="Cancel" press=".CancelDialog" type="Negative" />
            <Button text="Update" press=".UpdateDialog" type="Success" />
        </Toolbar>
    </Dialog>
</core:FragmentDefinition>
