		<ui:Table
							id="idShipReqsTable"
							rows="{eshipjetModel>/RecentShipmentSetShipReqLabel}"
							class="sapUiSmallMarginTop"
							selectionMode="None"
							visibleRowCount="13" >
							<ui:extension>
								<OverflowToolbar>
									<Button icon="sap-icon://filter" press="onShipReqFilterPopoverPress"></Button>
									<ToolbarSpacer ></ToolbarSpacer>
									<Button icon="sap-icon://action" press="onShipReqExportToExcel"></Button>
									<Button icon="sap-icon://action-settings" press="openShipReqColNamesPopover" >
										<customData>
											<core:CustomData key="remember" value="true" />
											<core:CustomData key="showClearButton" value="true" />
											<core:CustomData key="confirmButtonText" value="Ok" />
										</customData>
									</Button>										
								</OverflowToolbar>
							</ui:extension>

							<ui:columns>
								<ui:Column visible="{eshipjetModel>/ShipReqColumns/0/visible}" width="auto" >
									<Label text="Location Id" />
									<ui:template>
										<Text text="{eshipjetModel>RecCity}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/1/visible}" >
									<Label text="Location Name" />
									<ui:template>
										<Text text="{eshipjetModel>RecCity}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/2/visible}">
									<Label text="Consolidation Id" />
									<ui:template>
										<Text text="{eshipjetModel>Consolidation}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/3/visible}" >
									<Label text="Delivery/handling Requests" />
									<ui:template>
										<Text text="{eshipjetModel>requestIdLabelId}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/4/visible}" >
									<Label text="Eid" />
									<ui:template>
										<Text text="" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/5/visible}" >
									<Label text="Created Date" />
									<ui:template>
										<Text text="{eshipjetModel>Createddate}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/6/visible}" >
									<Label text="Ship Date" />
									<ui:template>
										<Text text="{eshipjetModel>DateAdded}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/7/visible}" >
									<Label text="Shipment Type" />
									<ui:template>
										<Text value="{eshipjetModel>Carriertype}" />
									</ui:template>
								</ui:Column>

								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/11/visible}" >
									<Label text="Ship Method Description" />
									<ui:template>
											<HBox justifyContent="SpaceBetween" alignItems="Center">
												<Text text="{eshipjetModel>CarrierCode}" wrapping="false" />
												<Button icon="sap-icon://add" press="onShipMethodUpdate" />
											</HBox>
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/12/visible}" >
									<Label text="Ship Description" />
									<ui:template>
										<Text text="{eshipjetModel>CarrierDesc}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/13/visible}" >
									<Label text="Service Name" />
									<ui:template>
										<Text text="{eshipjetModel>CarrierDesc}" wrapping="false" />
									</ui:template>
								</ui:Column>


								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/14/visible}" >
									<Label text="Master Tracking Number" />
									<ui:template>
										<Link text="{eshipjetModel>TrackingNumber}" press="onTrackingNumberPress" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/15/visible}" >
									<Label text="Tracking Number" />
									<ui:template>
										<Link text="{eshipjetModel>TrackingNumber}" press="onTrackingNumberPress" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/16/visible}" >
									<Label text="Quote Status" />
									<ui:template>
										<Text text="{eshipjetModel>DeliveryStatus}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/17/visible}" >
									<Label text="Ship To Contact" />
									<ui:template>
										<Text text="{eshipjetModel>RecContact}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/18/visible}" >
									<Label text="Ship To Company" />
									<ui:template>
										<Text text="{eshipjetModel>RecCompany}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/19/visible}" >
									<Label text="Ship To Address Line 1" />
									<ui:template>
										<Text text="{eshipjetModel>RecAddress1}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/20/visible}" >
									<Label text="Ship To State / Province" />
									<ui:template>
										<Text text="{eshipjetModel>RecRegion}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/21/visible}" >
									<Label text="Ship To City" />
									<ui:template>
										<Text text="{eshipjetModel>RecCity}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/22/visible}" >
									<Label text="Ship To Zip / Postal Code" />
									<ui:template>
										<Text text="{eshipjetModel>RecPostalcode}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/23/visible}" >
									<Label text="Ship To Country" />
									<ui:template>
										<Text text="{eshipjetModel>RecCountry}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/24/visible}" >
									<Label text="Ship To Phone No" />
									<ui:template>
										<Text text="{eshipjetModel>RecPhone}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/25/visible}" >
									<Label text="Ship To Email" />
									<ui:template>
										<Text text="{eshipjetModel>Emailaddress}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/26/visible}" >
									<Label text="Requester Name" />
									<ui:template>
										<Text text="{eshipjetModel>RecContact}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/27/visible}" >
									<Label text="Connected To" />
									<ui:template>
										<Text text="{}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/27/visible}" >
									<Label text="Status" />
									<ui:template>
										<Text text="{eshipjetModel>DeliveryStatus}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/27/visible}" >
									<Label text="Order Type" />
									<ui:template>
										<Text text="{eshipjetModel>Packagetype}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/27/visible}" >
									<Label text="Rfid" />
									<ui:template>
										<Text text="{}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column width="auto" visible="{eshipjetModel>/ShipReqColumns/27/visible}" >
									<Label text="Shipment Workflow" />
									<ui:template>
									<VBox justifyContent="Center" alignItems="Center">
											<Button icon="sap-icon://notes" press="onShipmentWorkflowPress"></Button>
									</VBox>
									
										<!-- <Text text="{eshipjetModel>Shipmenttype}" wrapping="false" /> -->
									</ui:template>
								</ui:Column>
								<ui:Column visible="true" width="4rem" >
									<Label text="Actions" />
									<ui:template>
										<HBox>
											<Button text="View Now" type="Transparent" press="onViewNowPressBackToShipNow" ></Button>
											<Button icon="sap-icon://megamenu" type="Transparent" press="handleDownArrowPress" ></Button>
										</HBox>
									</ui:template>
								</ui:Column>
							</ui:columns>
						</ui:Table>

</core:FragmentDefinition>
