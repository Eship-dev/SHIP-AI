<core:FragmentDefinition	xmlns="sap.m"    xmlns:ui="sap.ui.table"    	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"	xmlns:core="sap.ui.core">

	    <Toolbar width="auto"  height="15%" class="toolbarbottomborder fixedLayout" >
				<HBox class="sapUiTinyMarginBegin toolBarHeader">
					<Text text="Track Shipments: " class="toolBarHeader"/>
					<Text text=" 99 " class="highlightedText toolBarHeader" />
					<Text text=" in Last " class="toolBarHeader" />
					<Text text=" 7 " class="highlightedText toolBarHeader" />
					<Text text=" Days" class="toolBarHeader"/>
				</HBox>
				<ToolbarSpacer />
					<Text text="11/23/2024 - 11/29/2024" class="toolBarHeader1 sapUiTinyMarginBottom sapUiTinyMarginTop sapUiTinyMarginEnd " width="270px"></Text>
		</Toolbar>

		<Toolbar height="15%" >
			<HBox width="100%" >
				<VBox class="sapUiTinyMargin" width="25%">
					<Text text="3" class="NumberSizeTitle1"></Text>
					<Text text="Shipped" class="sapUiTinyMarginBottom"></Text>
					<Button type="Success" press="onTrackNowShippedFilterPress" class="shipped-request-button request-button" id="idShippedBtn" icon="false" width="100%" ></Button>
				</VBox>
				<VBox class="sapUiTinyMargin" width="25%">
					<Text text="3" class="NumberSizeTitle1"></Text>
					<Text text="In-Transit" class="sapUiTinyMarginBottom"></Text>
					<Button  press="onTrackNowInTransitFilterPress" class="cancelled-request-button request-button" id="idInTransitBtn" icon="false" width="100%"  ></Button>
				</VBox>
				<VBox class="sapUiTinyMargin" width="25%">
					<Text text="3" class="NumberSizeTitle1"></Text>
					<Text text="Cancelled" class="sapUiTinyMarginBottom"></Text>
					<Button type="Negative" press="onTrackNowCancelledFilterPress" id="idCancelledBtn" class="shippedProgress-request-button request-button" icon="false" width="100%" ></Button>
				</VBox>
				<VBox class="sapUiTinyMargin" width="25%">
					<Text text="3" class="NumberSizeTitle1"></Text>
					<Text text="Delivered" class="sapUiTinyMarginBottom"></Text>
					<Button type="Success" press="onTrackNowDeliveredFilterPress" id="idDeliveredBtn" class="inTransit-request-button request-button" icon="false" width="100%" ></Button>
				</VBox>
			</HBox>
		</Toolbar>

            <ui:Table
				id="idTrackNowTable"
				class="sapUiSmallMarginTop"
				selectionMode="None"
				visibleRowCount="12" >
				<ui:extension>
					<OverflowToolbar style="Clear">
						<Button icon="sap-icon://filter" press="onTrackNowFilterPress"></Button>
						<ToolbarSpacer />
						<Button icon="sap-icon://action"></Button>
					<Button icon="sap-icon://action-settings" press="openTrackNowColNamesPopover"></Button>
				</OverflowToolbar>
			</ui:extension>
			<ui:footer>
				<Toolbar >
					<ToolbarSpacer/>
					<Button  text="First" enabled="false" />
					<Button  text="Previous" enabled="false"/>
					<Text    text="1-50 of 74"/>
					<Button  text="Next" />
					<Button  text="Last" />
				</Toolbar>
			</ui:footer>
		</ui:Table>
</core:FragmentDefinition>
