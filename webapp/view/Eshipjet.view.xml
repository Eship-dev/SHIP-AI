<mvc:View
	controllerName="com.eshipjet.zeshipjet.controller.Eshipjet"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:tnt="sap.tnt"
	xmlns:ui="sap.ui.table"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	height="100%">
	<tnt:ToolPage id="toolPage">
		<tnt:header>
			<tnt:ToolHeader>
				<Button
					id="sideNavigationToggleButton"
					icon="sap-icon://menu2"
					type="Transparent"
					press=".onSideNavButtonPress">
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow" />
					</layoutData>
				</Button>
				<Text text="SAP ShipperCopilot 6.4"/>
				<ToolbarSpacer/>
				<Button icon="sap-icon://action-settings" press="onPressSettingsButton"/>
				<Button icon="sap-icon://bell"/>
			</tnt:ToolHeader>
		</tnt:header>
		<tnt:sideContent >
			<tnt:SideNavigation
			 	visible="{eshipjetModel>/SideNavigation}"
				expanded="true"
				selectedKey="{eshipjetModel>/selectedKey}"
				itemSelect=".onItemSelect">
				<tnt:NavigationList items="{path: 'eshipjetModel>/navigation'}">
					<tnt:NavigationListItem
						class="textBold"
						text="{eshipjetModel>title}"
						icon="{eshipjetModel>icon}"
						key="{eshipjetModel>key}">
					</tnt:NavigationListItem>
				</tnt:NavigationList>
			</tnt:SideNavigation>
		</tnt:sideContent>
		<tnt:mainContents>
			<NavContainer id="pageContainer" initialPage="Dashboard">
				<pages>
					<ScrollContainer
						id="Dashboard"
						horizontal="false"
						vertical="true"
						height="100%"
						class="sapUiContentPadding ship-tiles-page">
						<FlexBox justifyContent="Center" alignItems="Center" height="100%">	
							<l:HorizontalLayout id="TileContainerExpanded"
				                    allowWrapping="true"
				                    content="{eshipjetModel>/TileCollection}"
				                    class="sapUiTinyMargin ship-tile-container">
									<GenericTile press="onTilePress" class="ship-tile"
										headerImage="{eshipjetModel>image}">
										<TileContent class="ship-tile-content">
											<Button class="ship-tile-button" text="{eshipjetModel>title}"/>
										</TileContent>
									</GenericTile>
							</l:HorizontalLayout>
						</FlexBox>
					</ScrollContainer>
					<ScrollContainer
						id="ShipperCopilot"
						horizontal="false"
						vertical="true"
						height="100%"
						class="sapUiContentPadding ship-copilot-page">
									<VBox visible="{ShipperCopilotModel>/iconState}" class="ship-copilot-top-section">
										<HBox class="sapUiLargeMarginTop" justifyContent="Center">
											<Image src="{value: 'com/eshipjet/zeshipjet/css/images/copilotlogo.png',formatter: 'sap.ui.require.toUrl'}" width="5rem" height="5rem"/>
										</HBox>
										<HBox class="sapUiLargeMarginTop" justifyContent="Center">
											<Button class="sapUiTinyMargin ship-copilot-button all-shipments" icon="sap-icon://in-progress"  text="Show all Shipments"/>
											<Button class="sapUiTinyMargin ship-copilot-button country-shipments" icon="sap-icon://product" text="Coutry wise Shipments" />
											<Button class="sapUiTinyMargin ship-copilot-button open-order-status" icon="sap-icon://shipping-status" text="Orders with Open Status"/>
										</HBox>
									</VBox>
									<List id="chatList" items="{ShipperCopilotModel>/messages}" class="co-pilot-search-results" visible="{ShipperCopilotModel>/listState}" >
										<CustomListItem class="sapUiTinyMargin" >
											<HBox justifyContent="{= ${ShipperCopilotModel>sender} === 'You' ? 'End' : 'Start'}">
											<VBox class="messageBox {= ${ShipperCopilotModel>sender} === 'You' ? 'userMessage' : 'botMessage'}">
												<Text text="{ShipperCopilotModel>text}" wrapping="true" class="co-pilot-search-text" visible="{= ${ShipperCopilotModel>sender} === 'You'}" />
												<HBox>
												<Image class="sapUiSmallMarginEnd" src="{value: 'com/eshipjet/zeshipjet/css/images/copilotlogo.png',formatter: 'sap.ui.require.toUrl'}" width="16px" height="auto" visible="{= ${ShipperCopilotModel>sender} === 'BotError' || ${ShipperCopilotModel>sender} === 'Bot'}"/>
												<Text text="{ShipperCopilotModel>text}" wrapping="true" visible="{= ${ShipperCopilotModel>sender} === 'BotError'}" />
												<Image src="{ShipperCopilotModel>text}"
													width="70%" height="70%"
													press="onZoomImage" visible="{= ${ShipperCopilotModel>sender} === 'Bot'}" />
												</HBox>    
											</VBox>
											</HBox>
										</CustomListItem>
									</List>
									<HBox justifyContent="Center" class="ship-copilot-search-section">
										<Input id="userInput" class="sapUiTinyMarginEnd" placeholder="Good Afternoon Robert, I'm ready for the task" width="30rem" submit="onSendPress"/>
										<Button icon="sap-icon://arrow-top" press="onSendPress"/>
									</HBox>
					</ScrollContainer>

	<!-- Orders Starts here -->
			<ScrollContainer id="Orders" horizontal="false" vertical="true" height="100%" class="sapUiContentPadding">
				<core:Fragment fragmentName="com.eshipjet.zeshipjet.view.fragments.Orders" type="XML" />
			</ScrollContainer>

<!-- Ship Request Label Starts here -->
